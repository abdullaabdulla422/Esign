<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"

    android:orientation="vertical"
    >
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:gravity="center_vertical"
        android:weightSum="1"
        android:background="@color/material_blue_800"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/back"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@drawable/back"
            android:visibility="visible"
            tools:ignore="ContentDescription,RtlHardcoded" />

        <ImageView
            android:id="@+id/settings"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/rentalseting"
            android:visibility="gone"
            tools:ignore="ContentDescription,RtlHardcoded" />

        <TextView
            android:id="@+id/rentalcompanyname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:maxEms="10"
            android:layout_weight="1"
            android:textStyle="bold"
            android:textColor="@android:color/white"
            android:paddingLeft="20dp"
            android:singleLine="true"
            android:text="CompanyName"
            android:textSize="24dp"
            tools:ignore="Deprecated,HardcodedText,RtlHardcoded,RtlSymmetry,SpUsage" />


        <TextView
            android:id="@+id/orderno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/rentalcompanyname"
            android:layout_alignBottom="@+id/rentalcompanyname"
            android:layout_toLeftOf="@+id/settings"
            android:paddingRight="20dp"
            android:layout_toRightOf="@id/back"

            android:textColor="@android:color/white"
            android:singleLine="true"
            android:text=""
            android:textSize="24dp"
            tools:ignore="Deprecated,RelativeOverlap,RtlHardcoded,RtlSymmetry,SpUsage" />


    </RelativeLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/txt_companyname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:singleLine="true"
            android:textColor="@color/black"

            android:textSize="24dp"
            android:text="CompanyName"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="Deprecated,HardcodedText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/txt_contact"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:textColor="@color/black"

            android:text="Contact"
            android:textSize="24dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/txt_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:gravity="right"
            android:textSize="24dp"
            android:textColor="@color/black"

            android:text="Date"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="HardcodedText,RtlHardcoded" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/txt_address1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:textColor="@color/black"

            android:text="Address L1"
            android:textSize="24dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="HardcodedText" />
        <!--
        <TextView
            android:id="@+id/txt_branch"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:gravity="right"
            android:text="Branch"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        -->
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/txt_address2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:textSize="24dp"
            android:text="Address L2"
            android:textColor="@color/black"

            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/txt_rental_id"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:gravity="right"
            android:textSize="24dp"
            android:text="RentalID"
            android:textColor="@color/black"

            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="HardcodedText,RtlHardcoded" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/txt_location"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:textSize="24dp"
            android:textColor="@color/black"

            android:text="City,  State,  Zip"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/txt_sales_man"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:textColor="@color/black"

            android:gravity="right"
            android:text="Sales Rep"
            android:textSize="24dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="HardcodedText,RtlHardcoded" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/txt_phone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:text="Phone"
            android:textColor="@color/black"

            android:textSize="24dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="HardcodedText" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:gravity="left"
            android:textSize="24dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            tools:ignore="RtlHardcoded" />
    </LinearLayout>

    <!--
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/txt_datecurrent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:layout_weight="1"
            android:text="Date"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/current_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/bottom_margin"
            android:gravity="left"
            android:textAppearance="?android:attr/textAppearanceMedium" />
    </LinearLayout>
    -->


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000000" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1"
            tools:ignore="InefficientWeight">

    <ListView
        android:id="@+id/listView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
         >
    </ListView>
        </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"

        android:gravity="center"
        android:layout_gravity="bottom"
        android:orientation="vertical" >


        <com.ebs.rental.uidesigns.MaterialRippleLayout
            android:layout_width="250dp"
            android:layout_height="60dp"
            style="@style/RippleStyle"
            android:layout_gravity="center"
            android:layout_weight="1"
            >


        <Button
            android:id="@+id/btncheck"
            style="@style/btnStyleGenoa"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/round_button_corner"
            android:onClick="click"
            android:text="@string/title_activity_rental_list_return"
            android:textSize="23sp"
            android:textColor="@android:color/white" />
        </com.ebs.rental.uidesigns.MaterialRippleLayout>
    </LinearLayout>

</LinearLayout>