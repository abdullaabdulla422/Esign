        android:background="#000000" /> -->

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_weight="13"
        android:gravity="center"
        android:orientation="horizontal"
        tools:ignore="InefficientWeight">

        <com.ebs.rental.uidesigns.MaterialRippleLayout

            style="@style/RippleStyle"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:layout_gravity="center"

            android:layout_margin="10dp">

            <Button
                android:id="@+id/btncancelsummary"
                style="@style/btnStyleGenoa"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="1dp"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:textSize="12sp"
                android:text="Cancel"
                android:background="@drawable/round_button_corner"

                tools:ignore="ButtonOrder,HardcodedText,ObsoleteLayoutParam" />
        </com.ebs.rental.uidesigns.MaterialRippleLayout>
        <com.ebs.rental.uidesigns.MaterialRippleLayout

            style="@style/RippleStyle"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:layout_margin="10dp">

        <Button
            android:id="@+id/btnreviewsummary"
            style="@style/btnStyleGenoa"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="1dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:background="@drawable/round_button_corner"

            android:textSize="12sp"
            android:text="Review"
            tools:ignore="HardcodedText,ObsoleteLayoutParam,RtlHardcoded" />
        </com.ebs.rental.uidesigns.MaterialRippleLayout>

    </LinearLayout>

