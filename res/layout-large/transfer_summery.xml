                android:orientation="horizontal" >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/bottom_margin"
                    android:layout_weight=".5"
                    android:textColor="@color/black"
                    android:text="Transfer To :"
                    android:textSize="16dp"
                    tools:ignore="HardcodedText,SpUsage" />

                <TextView
                    android:id="@+id/customer_reciving_branch"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/bottom_margin"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="16dp"

                    android:text=""
                    tools:ignore="SpUsage" />
            </LinearLayout>








        </LinearLayout>

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical"
            android:weightSum="1">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="58dp"
                        android:orientation="horizontal"
                        android:visibility="gone"
                        android:weightSum="10">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="8"
                            android:gravity="center"
                            android:paddingLeft="40dp"
                            android:text="Summary"
                            android:textColor="@android:color/black"
                            android:textSize="22sp" />

                        <ImageView
                            android:id="@+id/img_button"
                            android:layout_width="30dp"
                            android:layout_height="30dp"

                            android:layout_gravity="right|center"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/close"
                            android:contentDescription="@string/cancel" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:weightSum="10">

                        <LinearLayout
                            android:id="@+id/layout_global"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:background="@color/intake_bg"
                            android:orientation="horizontal"
                            android:padding="1dp"
                            android:weightSum="1">


                            <TextView
                                android:id="@+id/walk_around"
                                android:layout_width="150dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".5"
                                android:background="@drawable/round_button_corner"
                                android:gravity="center"
                                android:onClick="true"
                                android:paddingLeft="10dp"
                                android:paddingTop="5dp"
                                android:paddingRight="10dp"
                                android:paddingBottom="5dp"
                                android:text="Walk Around"
                                android:textColor="#fff"
                                android:textStyle="bold"
                                tools:ignore="OnClick" />

                            <View
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:background="@color/intake_bg" />

                            <TextView
                                android:id="@+id/check_list"
                                android:layout_width="150dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".5"
                                android:background="@drawable/round_button_corner"
                                android:checked="false"
                                android:gravity="center"
                                android:onClick="true"
                                android:paddingLeft="10dp"
                                android:paddingTop="5dp"
                                android:paddingRight="10dp"
                                android:paddingBottom="5dp"
                                android:text="Check List"
                                android:textColor="#fff"
                                android:textStyle="bold"
                                tools:ignore="OnClick" />


                        </LinearLayout>


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <ListView
                            android:id="@+id/listViewsummary"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"></ListView>

                    </LinearLayout>

                    <!--  <ImageButton
                           android:id="@+id/img_button"
                           android:layout_width="60dp"
                           android:layout_weight="2"
                           android:layout_height="wrap_content"
                           android:layout_gravity="right|center"
                           android:contentDescription="@string/cancel"
            
                           android:src="@drawable/exit_button" /> -->

                </LinearLayout>




            </LinearLayout>






        </LinearLayout>




    </LinearLayout>

