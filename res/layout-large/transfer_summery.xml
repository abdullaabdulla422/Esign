                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/bottom_margin"
                android:layout_weight="1"
                android:textSize="16dp"
                android:textColor="@color/black"
                android:text=""
                tools:ignore="SpUsage" />
        </LinearLayout>

